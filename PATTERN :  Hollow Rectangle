class Main {
    public static void main(String[] args) {
        System.out.println("Hello, World!\n");
        int row=5,col=7;
        for(int i=1;i<=row;i++)
        {
            for(int j=1;j<=col;j++)
        {
            if(i==1 || i==row || j==col || j==1 )
            System.out.print("*");
           else
           System.out.print(" ");
        }
        System.out.print("\n");
        }
    }
}
